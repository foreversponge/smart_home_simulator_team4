<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootanchorpane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HouseLayoutController">
    <children>
      <Label layoutX="90.0" layoutY="79.0" text="Smart Home Simulator">
         <font>
            <Font name="SansSerif Bold" size="39.0" />
         </font>
      </Label>
      <Pane layoutX="36.0" layoutY="167.0" prefHeight="200.0" prefWidth="518.0">
         <children>
                      <Label fx:id="pathToFileLabel" alignment="BASELINE_LEFT" layoutX="117.0" layoutY="55.0" prefHeight="32.0" prefWidth="261.0" style="-fx-border-color: #000000; -fx-border-radius: 5;">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="filePathString" alignment="BASELINE_LEFT" layoutX="30.0" layoutY="55.0" prefHeight="32.0" prefWidth="95.0" text="File Path:">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label fx:id="subtitleLabel" alignment="BASELINE_LEFT" layoutX="96.0" layoutY="-2.0" prefHeight="32.0" prefWidth="326.0" text="Please upload your house layout file.">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <JFXButton fx:id="continueButton" disable="true" layoutX="200.0" layoutY="135.0" onMouseClicked="#onContinueClick" prefHeight="27.0" prefWidth="118.0" style="-fx-border-color: #000000; -fx-border-radius: 5;" text="Continue" />
            <JFXButton fx:id="uploadButton" layoutX="401.0" layoutY="58.0" onMouseClicked="#onUploadClick" prefHeight="27.0" prefWidth="86.0" style="-fx-border-color: #000000; -fx-border-radius: 5;" text="Upload" />
         </children>
      </Pane>
    </children>
</AnchorPane>
