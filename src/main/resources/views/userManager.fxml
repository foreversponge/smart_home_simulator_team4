<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXButton?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.UserManagerController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableView" layoutX="44.0" layoutY="40.0" prefHeight="315.0" prefWidth="327.0">
            <columns>
                <TableColumn fx:id="tableColumnName" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="tableColumnStatus" prefWidth="75.0" text="Status" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <JFXButton layoutX="425.0" layoutY="101.0" onAction="#handleAdd" text="Add" />
        <JFXButton layoutX="425.0" layoutY="149.0" onAction="#handleEdit" text="Edit" />
        <JFXButton layoutX="418.0" layoutY="200.0" onAction="#handleDelete" text="Delete" />
        <JFXButton layoutX="418.0" layoutY="315.0" onAction="#handleCancel" text="Cancel" />
        <JFXButton layoutX="423.0" layoutY="255.0" onAction="#handleSave" text="save" />
        <Label fx:id="errorLabel" layoutX="404.0" layoutY="49.0" prefHeight="17.0" prefWidth="149.0" />
    </children>
</AnchorPane>
